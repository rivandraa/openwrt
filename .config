# Target System
CONFIG_TARGET_realtek=y
CONFIG_TARGET_realtek_RTL8197FS=y
CONFIG_TARGET_realtek_RTL8197FS_DEVICE_HG680P=y

# Base System
CONFIG_PACKAGE_base-files=y
CONFIG_PACKAGE_busybox=y
CONFIG_PACKAGE_dropbear=y
CONFIG_PACKAGE_fstools=y
CONFIG_PACKAGE_netifd=y
CONFIG_PACKAGE_openwrt-keyring=y
CONFIG_PACKAGE_opkg=y
CONFIG_PACKAGE_uboot-envtools=y
CONFIG_PACKAGE_uci=y
CONFIG_PACKAGE_uclient-fetch=y

# Kernel Modules
CONFIG_PACKAGE_kmod-gpio-button-hotplug=y
CONFIG_PACKAGE_kmod-leds-gpio=y
CONFIG_PACKAGE_kmod-usb-core=y
CONFIG_PACKAGE_kmod-usb-ohci=y
CONFIG_PACKAGE_kmod-usb2=y
CONFIG_PACKAGE_kmod-usb3=y

# Network
CONFIG_PACKAGE_dnsmasq=y
CONFIG_PACKAGE_firewall4=y
CONFIG_PACKAGE_iptables=y
CONFIG_PACKAGE_iwinfo=y
CONFIG_PACKAGE_kmod-ipt-offload=y
CONFIG_PACKAGE_miniupnpd=y
CONFIG_PACKAGE_openssh-sftp-server=y
CONFIG_PACKAGE_ppp=y
CONFIG_PACKAGE_ppp-mod-pppoe=y

# LuCI (Web Interface)
CONFIG_PACKAGE_luci=y
CONFIG_PACKAGE_luci-app-firewall=y
CONFIG_PACKAGE_luci-app-opkg=y
CONFIG_PACKAGE_luci-base=y
CONFIG_PACKAGE_luci-mod-admin-full=y
CONFIG_PACKAGE_luci-theme-bootstrap=y

# Wireless Support (Optional)
CONFIG_PACKAGE_kmod-mt76x2u=y  # Wireless driver, sesuaikan dengan chip yang ada

# Additional Utilities
CONFIG_PACKAGE_htop=y
CONFIG_PACKAGE_wget=y
CONFIG_PACKAGE_ca-certificates=y
CONFIG_PACKAGE_ip-full=y
CONFIG_PACKAGE_ifstat=y
CONFIG_PACKAGE_nftables=y

# USB Support
CONFIG_PACKAGE_block-mount=y
CONFIG_PACKAGE_kmod-fs-ext4=y
CONFIG_PACKAGE_kmod-fs-vfat=y
CONFIG_PACKAGE_kmod-usb-storage=y
CONFIG_PACKAGE_kmod-usb-storage-extras=y
CONFIG_PACKAGE_kmod-usb-storage-uas=y

# Optional (Proxy or VPN Support)
CONFIG_PACKAGE_openvpn-openssl=y
CONFIG_PACKAGE_trojan-go=y
CONFIG_PACKAGE_v2ray-core=y
CONFIG_PACKAGE_kmod-tun=y

# Other useful packages (adjust as needed)
CONFIG_PACKAGE_luci-app-sqm=y
CONFIG_PACKAGE_luci-app-adblock=y
